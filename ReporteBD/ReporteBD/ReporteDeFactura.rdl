  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>RestauranteDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idVentas">
            <Value>=Parameters!idVentas.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Ventas.idVenta, Ventas.fechadeVenta, Ventas.totalPagar, ProductosVenta.nombre, DetallesVenta.precioventa, DetallesVenta.cantidad, DetallesVenta.totalProducto
FROM            Ventas INNER JOIN
                         DetallesVenta ON Ventas.idVenta = DetallesVenta.idVenta INNER JOIN
                         ProductosVenta ON DetallesVenta.idProductoV = ProductosVenta.idProductoV
WHERE        (Ventas.idVenta = @idVentas)</CommandText>
      </Query>
      <Fields>
        <Field Name="idVenta">
          <DataField>idVenta</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="fechadeVenta">
          <DataField>fechadeVenta</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="totalPagar">
